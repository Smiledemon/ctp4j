package com.itqy8.ctp.md.dao;

import com.itqy8.ctp.md.entity.MarketData30s;
import com.itqy8.framework.dao.BaseDaoImpl;

/**
 * 
 * @author meixinbin
 *
 */
public class MarketData30sDaoImpl extends BaseDaoImpl implements MarketData30sDao{

	private static final long serialVersionUID = 1L;

	@Override
	public MarketData30s getMarketData(String instrumentID, Long priceTime) {
		String hql = "from MarketData30s md where md.instrumentId=:instrumentId and md.priceTime=:priceTime";
		return (MarketData30s) getCurrentSession().createQuery(hql).setString("instrumentId", instrumentID).setLong("priceTime", priceTime).uniqueResult();
	}

}
